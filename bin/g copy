#!/bin/bash

my_current_repo=`g_get_repo.py`

case "$my_current_repo" in

"tfa")
	source build_tfa.sh
	eval _"$@"
	;;
 "optee")
	source build_optee.sh
	eval _"$@"
	;;
"uboot")
	source build_uboot.sh
	eval _$@
	;;
"kernel")
	source build_kernel.sh $@
	;;
"tfm")
	source build_tfm.sh $@
	eval _$@
	;;
esac