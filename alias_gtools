alias tfa='cd tfa 2>/dev/null; cd ../tfa'
alias tfm='cd tfm 2>/dev/null; cd ../tfm'
alias optee='cd optee 2>/dev/null; cd ../optee'
alias uboot='cd uboot 2>/dev/null; cd ../uboot'
alias kernel='cd kernel 2>/dev/null; cd ../kernel'
alias dt='cd dt 2>/dev/null; cd ../dt'
alias scp='cd scp 2>/dev/null; cd ../scp'

alias v='test -s .conf && board=`cat .conf` || true &&  test -s ../.conf && board=`cat ../.conf` && source $board'
